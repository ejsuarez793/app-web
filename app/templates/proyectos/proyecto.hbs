{{logged-header-component currentName=currentName }}
 <div class="container-fluid">
        <div class="row">
        
          <div class="col-lg-2  ">
            <h2>Proyecto</h2>
          </div>
          <div class="col-lg-10">
            <div id="alertMsg" class="alert alert-dismissible" role="alert" hidden>
              <button type="button" class="close" {{action "cerrarMsg"}} aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <strong>{{msg.tipo}}</strong> {{msg.desc}}
            </div>
          </div>
        </div>

        <div class="row ">
			    {{coordinador-sidebar-component}}
          <div class="col-md-10">
          <div>

            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
              <li role="presentation" class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
              <li role="presentation"><a href="#etapas" aria-controls="etapas" role="tab" data-toggle="tab">Etapas</a></li>
              <li role="presentation"><a href="#listado" aria-controls="listado" role="tab" data-toggle="tab">Listado Materiales</a></li>
              <li role="presentation"><a href="#solicitudesmat" aria-controls="solicitudesmat" role="tab" data-toggle="tab">Solicitudes de Material</a></li>
              <li role="presentation"><a href="#presupuesto" aria-controls="presupuesto" role="tab" data-toggle="tab">Presupuesto(s)</a></li>
            </ul>

            <!-- Tab panes -->
            <div class="tab-content">

              <!-- GENERAL-->
              <div role="tabpanel" class="tab-pane active" id="general">
                

              <div class="well jumbo-color">
                <div class="row">
                  <div class="col-md-12 ">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                          <h3 class="panel-title">Detalles Proyecto {{proyecto.estatus}} 
                          
                            <small class="bg-info">{{proyecto.f_est}}</small>  
                            <small class="bg-success">{{proyecto.f_ini}}</small>
                            <small class="bg-warning">{{proyecto.f_fin}}</small> 

                          </h3>
                        </div>
                        <div class="panel-body">
                        
                          <div class="row">
                            <div class="col-sm-6">
                              <h4>{{proyecto.nombre}}</h4>
                              <h4><small>{{proyecto.codigo}}</small></h4>
                              <h4><small>{{proyecto.ubicacion}}</small></h4>
                              <p class="text-justify">{{proyecto.desc}}</p>
                            </div>
                            <div class="col-sm-6">
                            </div>

                          </div>

                        </div>
                        <div class="panel-footer">
                          <div class="row">
                          <div class="col-sm-9"></div>
                            <div class="col-sm-3">
                              <button class="btn btn-primary" {{action 'openModalGeneral'}} type="submit">Editar</button>
                              <button class="btn btn-primary" {{action 'openModalPresupuesto' false}} type="submit">{{accion}}</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>


              </div>

              <!--ETAPAS-->
              <div role="tabpanel" class="tab-pane" id="etapas">
                <div class="well jumbo-color">
                <div class="row">
                  <div class="col-md-12 ">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                          <h3 class="panel-title">Etapas Proyecto</h3>
                        </div>
                        <div class="panel-body">
                        
                          <div class="row">
                            <div class="col-sm-12">
                               <table class="table table-bordered table-hover " id="tabla">  
                                <thead class="header-tabla">
                                  <tr>
                                    <th class="col-md-2">Código</th>
                                    <th >Nombre</th>
                                    <th >Estado</th>
                                    <th class="col-md-1">Reporte Detalle</th>
                                    <th class="col-md-1">Reportes</th>
                                    <th class="col-md-1">Actividades</th>
                                    <th class="col-md-1">Editar</th>
                                  </tr>
                                </thead>
                                <tbody id="tbody" class="text-center">
                                  {{#each proyecto.etapas as |etapa|}}
                                  <tr>
                                    <td class="col-tabla">{{etapa.letra}}</td>
                                    <td class="col-tabla"><{{etapa.nombre}}</td>
                                    <td class="col-tabla">{{etapa.estatus}}</td>
                                    <td >
                                      <button {{action 'openModalReporteDetalle' etapa}} class="btn btn-default btn-xs">ver</button>
                                    </td>
                                    <td >
                                      <button {{action 'openModalReportes' etapa}} class="btn btn-default btn-xs">ver</button>
                                    </td>
                                    <td >
                                      <button {{action 'openModalActividades' etapa}} class="btn btn-default btn-xs">ver</button>
                                    </td>
                                    <td >
                                      <button {{action 'openModalEtapa' etapa true}} class="btn btn-default btn-xs">editar</button>
                                    </td>
                                  </tr>
                                  {{/each}}
                                </tbody>
                                </table>
                            </div>
                          </div>

                        </div>
                        <div class="panel-footer">
                          <div class="row">
                            <div class="col-sm-9"></div>
                            <div class="col-sm-3">
                              <button class="btn btn-primary" {{action 'llenarReporteDetalle'}} type="submit">Llenar reporte detalle</button>
                              <button class="btn btn-primary" {{action 'openModalEtapa' null false}} type="submit">Crear Etapa</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div role="tabpanel" class="tab-pane" id="listado">listado</div>
              <div role="tabpanel" class="tab-pane" id="solicitudesmat">solicitudesmat</div>

              <!--PRESUPUESTOS-->
              <div role="tabpanel" class="tab-pane" id="presupuesto">

              <div class="row">

                <div class="col-lg-9">
                
                </div>

                <div class="col-lg-3">
                  <h6>
                    <button class="btn btn-primary" {{action 'openModalPresupuesto' false}} type="submit">Solicitar Presupuesto</button>
                  </h6>
                </div>

                <div class="col-md-12">
                <table class="table table-bordered table-hover " id="tabla">  
                  <thead class="header-tabla">
                    <tr>
                      <th class="col-md-2" {{action "ordenarPor" "codigo" true}}>Código<span id="th-codigo" class="glyphicon glyphicon-chevron-down"></span></th>
                      <th {{action "ordenarPor" "desc" true}}>Descripción<span id="th-desc" class="glyphicon glyphicon-chevron-down"></span></th>
                      <th class="col-md-1"{{action "ordenarPor" "fecha" true}}>Fecha<span id="th-fecha" class="glyphicon glyphicon-chevron-down"></span></th>
                      <th class="col-md-1"{{action "ordenarPor" "estatus" true}}>Estado<span id="th-estatus" class="glyphicon glyphicon-chevron-down"></span></th>
                      <th class="col-md-1">Detalles</th>
                    </tr>
                  </thead>
                  <tbody id="tbody" class="text-center">
                    {{#each proyecto.presupuestos as |presupuesto|}}
                    <tr>
                      <td class="col-tabla">{{presupuesto.codigo}}</td>
                      <td class="col-tabla"><{{presupuesto.desc}}</td>
                      <td class="col-tabla">{{presupuesto.fecha}}</td>
                      <td class="col-tabla">{{presupuesto.estatus}}</td>
                      <td ><button {{action 'openModalPresupuesto' true presupuesto}} class="btn btn-default btn-xs">Abrir</button></td>
                    </tr>
                    {{/each}}
                  </tbody>
                </table>
                </div>

              </div>

              </div>

          </div>



          </div>
          </div>
		    </div>
</div>










<!-- Modal GENERAL-->
<div class="modal fade " id="myModalGeneral" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Edicion Detalles Generales</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
          <form  id="formulario_pg">
            <div class="row">
              <div class="col-md-12">
            <div class="form-group">
              <label>Nombre </label><small class="form-text text-muted">* </small>
              <div class="input-group">
                {{input name="nombre" id="nombre_pg" type="text" class="form-control" placeholder="Nombre del Proyecto" value=pg.nombre}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>

            </div>
          </div>

          <div class="col-md-12">
            <div class="form-group">
              <label>Descripción </label><small class="form-text text-muted">* </small>
              <div class="input-group">
                {{textarea name="desc" id="desc_pg" type="text" class="form-control" placeholder="Descripción general del proyecto" value=pg.desc}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>

            </div>
          </div>

          
          <div class="col-md-6">
          <div class="form-group">
            <label>Ubicación</label><small class="form-text text-muted"> </small>
            <div class="input-group">
              {{input name="ubicacion" type="text" class="form-control" placeholder="Ubicación del Proyecto" value=pg.ubicacion}}
              <span class="input-group-addon">{{fa-icon "map-marker"}}</span>
            </div>
          </div>
          </div>

          <div class="col-md-6">
          <div class="form-group">
            <label>Fecha estimada </label><small class="form-text text-muted">*</small>
            <div class="input-group">
              {{input name="f_est" type="date" class="form-control" placeholder="Fecha estimada de finalización" value=pg.f_est}}
              <span class="input-group-addon">{{fa-icon "calendar"}}</span>
            </div>
          </div>
          </div>

        



        </div>
      </form>
        </div>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
        <button class="btn btn-primary" {{action 'guardarProyectoGeneral'}}>Guardar</button>
      </div>
    </div>
  </div>
</div>










<!-- Modal Etapas-->
<div class="modal fade " id="myModalEtapa" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Etapa</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
          <form  id="formulario_pe">
            <div class="row">

            <div class="col-md-6">
            <div class="form-group">
              <label>Letra </label><small class="form-text text-muted">* </small>
              <div class="input-group">
                {{input name="nombre" type="text" class="form-control" value=pe.letra disabled="true"}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>

            </div>
            </div>

            <div class="col-md-6">
            <div class="form-group">
              <label>Fecha Estimada </label><small class="form-text text-muted">*</small>
              <div class="input-group">
                {{input name="f_est" type="date" class="form-control" value=pe.f_est}}
                <span class="input-group-addon">{{fa-icon "calendar"}}</span>
              </div>
            </div>
            </div>

            <div class="col-md-12">
            <div class="form-group">
              <label>Nombre </label><small class="form-text text-muted">* </small>
              <div class="input-group">
                {{input name="nombre" type="text" class="form-control" placeholder="Nombre de la etapa" value=pe.nombre}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>

            </div>
            </div>


              </div>
            </form>
          </div>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
        <button class="btn btn-primary" {{action 'guardarPE'}}>Guardar</button>
      </div>
    </div>
  </div>
</div>






<!-- Modal REPORTE DETALLE-->
<div class="modal fade " id="myModalReporteDetalle" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Reporte Detalle</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
          

            <div class="col-md-6 ">
              <div class="panel panel-primary">
                <div class="panel-heading">
                  <h3 class="panel-title">Persona que atendió visita</h3>
                </div>
                <div class="panel-body">
                  <dl class="dl-horizontal">
                    <dt>Nombre - Cargo</dt> <dd>{{reporte_detalle.persona_a}} - {{reporte_detalle.cargo_a}}</dd>
                  </dl> 
                </div>
              </div>
            </div>

            <div class="col-md-6 "> 
              <div class="panel panel-primary">
                <div class="panel-heading">
                  <h3 class="panel-title">Técnico</h3>
                </div>
                <div class="panel-body">
                  <dl class="dl-horizontal">
                    <dt>Nombre</dt> <dd>{{reporte_detalle.nombre_t}}</dd>   
                  </dl>
                </div>
              </div>
            </div>

            <div class="col-md-12 ">
              <div class="panel panel-primary">
                <div class="panel-heading">
                  <h3 class="panel-title">Observaciones</h3>
                </div>
                <div class="panel-body"> 
                  <p>{{reporte_detalle.observ}}</p>
                </div>
              </div>
            </div>

              <div class="col-md-12 ">
              <div class="panel panel-primary">
                <div class="panel-heading">
                  <h3 class="panel-title">Vicios Ocultos</h3>
                </div>
                <div class="panel-body">
                  <p>{{reporte_detalle.vicios_ocu}}</p>
                </div>
              </div>
            </div>







        </div>

      </div>
    </div>
  </div>
</div>



<!-- Modal REPORTES-->
<div class="modal fade " id="myModalReportes" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Reportes</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
          <ul class="list-group">
          {{#each etapa.reportes as |reporte|}}
            <li data-toggle="collapse" data-target="#collapse{{reporte.codigo}}" aria-expanded="false" aria-controls="collapse{{reporte.codigo}}" class="list-group-item">

  
            <strong>{{reporte.nombre_t}}</strong>
            <small class="text-muted">{{reporte.fecha}}</small>
            <strong class="text-success">{{reporte.estado}}</strong>


            <span class="badge">{{reporte.tipo}}</span>
            </li>
            <div class="collapse" id="collapse{{reporte.codigo}}">
              <div class="well">
                    {{reporte.observ}}
              </div>
              <div class="row">
                <div class="col-sm-10"></div>
                <div class="col-sm-2">
                  <input type="checkbox" value="{{reporte.codigo}}"><span> Leido</span>
                </div>
              </div>
            </div>
          {{/each}}
          </ul>  
        </div>
        </div>

      </div>
      <div class="modal-footer">
        <button class="btn btn-primary" {{action 'guardarReportesLeidos'}}>Marcar Leidos</button>
        <button class="btn btn-primary" {{action 'crearReporte' proyecto.etapas}}>Guardar</button>
      </div>
    </div>
  </div>
</div>





<!-- Modal Actividades-->
<div class="modal fade " id="myModalActividades" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Actividades</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
          {{#if sin_actividades}}
             <div class="page-header">
                  <h3>Definición de actividades</h3>
                  <p>Esta etapa no tiene actividades definidas, a continuación deberá definirlas según se requiera y basándose en el contenido del reporte de detalle, una vez que finalice debe oprimir el botón guardar. <strong> Esta definición solo puede hacerce una vez. No se puede editar las actividades despues de guardadas</strong></p>
              </div>

                <ul class="list-group">
                  {{#each nuevas_actividades as |actividad|}}
                    <li data-toggle="collapse" data-target="#collapse_act{{actividad.nro}}" aria-expanded="false" aria-controls="collapse_act{{actividad.nro}}" class="list-group-item">
                    <button type="button" class="close" aria-label="Close" {{action 'eliminarActividad' actividad bubbles=false}}>
                      <span aria-hidden="true">&times;</span>
                    </button>
                    <strong> Actividad Nro: {{actividad.nro}}</strong>
                    </li>
                    <div class="collapse" id="collapse_act{{actividad.nro}}">
                      <div class="well">
                            {{actividad.desc}}
                      </div>
                    </div>
                  {{/each}}
                </ul>  
                <form id="formulario_na">
                <div class="row">
                  <div class="col-md-2">
                    <div class="form-group">
                    <label>Nro </label><small class="form-text text-muted"></small>
                    <div class="input-group">
                      {{input name="nro" type="text" class="form-control" value=na.nro disabled="true"}}
                    </div>
                    </div>
                  </div>

                  <div class="col-md-10">
                    <div class="form-group">
                    <label>Descripción </label><small class="form-text text-muted"></small>
                    <div class="input-group">
                      {{input name="desc" type="text" class="form-control" value=na.desc}}
                      <span class="input-group-addon">{{fa-icon "bars"}}</span>
                    </div>

                    </div>
                  </div>

                </div>
                </form>
              <button class="btn btn-primary pull-right" {{action 'agregarActividad'}}>Añadir Actividad</button>
          {{else}}
             <ul class="list-group">
                  {{#each etapa.actividades as |actividad|}}
                    <li data-toggle="collapse" data-target="#collapse_act{{actividad.nro}}" aria-expanded="false" aria-controls="collapse_act{{actividad.nro}}" class="list-group-item">
                    <strong> Actividad Nro: {{actividad.nro}}</strong>
                    {{#if actividad.completada}}
                      <span class="badge">Completada</span>
                    {{else}}
                      <span class="badge">Pendiente</span>
                    {{/if}}
                    </li>
                    <div class="collapse" id="collapse_act{{actividad.nro}}">
                      <div class="well">
                            {{actividad.desc}}
                      </div>
                    </div>
                  {{/each}}
              </ul>  
          {{/if}}
        </div>
        </div>

      </div>
      <div class="modal-footer">
        <button class="btn btn-primary" {{action 'guardarActividades'}}>Guardar</button>
      </div>
    </div>
  </div>
</div>
















<!-- Modal Presupuesto-->
<div class="modal fade " id="myModalPresupuesto" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalPresupuestoLabel">Presupuesto</h4>
        <small class="form-text text-muted"></small>
      </div>

      <div class="modal-body">
        <div class="container-fluid">
          <div class="row">

            <div class="col-md-12 well">
            <div class="col-md-8">
              <ul class="ul-presupuesto">
                <li><b>Sistelred, C.A</b></li>
                <li><b>Rif J-30994445-2</b></li>
                <li>Caracas, {{presupuesto.fecha_m}}</li>
                <li><b>Cotización a:</b> {{proyecto.cliente.rif}}, {{proyecto.cliente.nombre}}, Tlf:{{proyecto.cliente.tlf1}}</li>
                <br>
              </ul>
            </div>
            <div class="col-md-4">
            <ul class="ul-presupuesto">
                <li ><b>Cotización Nro:</b> {{presupuesto.codigo}}</li>
            </ul>
            </div>
            <div class="col-md-12">
              <ul class="text-center ul-presupuesto">
                <li ><b>Atención: </b> {{presupuesto.atencion_n}}, <b>Email: </b>{{presupuesto.atencion_e}}</li>
              </ul>
            </div>
            <div class="col-md-12">
              <ul class=" ul-presupuesto">
                <li >{{presupuesto.desc}}</li>
              </ul>
            </div>
            </div>
            <div class="col-md-12 well">
               <div class="col-md-12">
               <table class="table table-bordered table-hover " id="tabla">  
                  <thead class="header-tabla">
                    <tr>
                      <th class="col-md-1" {{action "ordenarPor" "codigo" false}}>Código<span id="th-codigo1" class="glyphicon glyphicon-chevron-down"></span></th>
                      <th>Descripción</th>
                      <th class="col-md-1">Cantidad</th>
                      <th class="col-md-2">Precio Unitario</th>
                      <th class="col-md-2" {{action "ordenarPor" "precio_total"}}>Precio Total</th>
                      <th class="col-md-1">Selección</th>
                    </tr>
                  </thead>
                  <tbody id="tbody" class="text-center">
                    {{#each ecs as |elemento|}}
                    <tr >
                      <td class="col-tabla">{{elemento.codigo}}</td>
                      <td class="col-tabla"><{{elemento.desc}}</td>
                      <td class="col-tabla">
                        <input type="number" class="form-control input-sm" id="cantidad_{{elemento.codigo}}" value="{{elemento.cantidad}}"  min="0" {{action 'calcularPrecioTotal' elemento.codigo on='change'}}>
                      </td>
                      <td class="col-tabla">
                        <input type="number" class="form-control input-sm" id="pu_{{elemento.codigo}}" value="{{elemento.precio_act}}"  min="0" {{action 'calcularPrecioTotal' elemento.codigo on='change'}}>
                      </td>
                      <td class="col-tabla" name="precio_total" id="pt_{{elemento.codigo}}"  value="{{elemento.precio_total}}" >{{elemento.precio_total}}</td>
                      <td><input type="checkbox" value={{elemento.codigo}}></td>
                    </tr>
                    {{/each}}
                  </tbody>
                </table>
                </div>
                <div class="col-md-2">
                </div>
                <div class="col-md-3">
                  <h2>
                    <div class="input-group" aria-describedby="basic-addon2">
                    <span class="input-group-addon" id="basic-addon2">Tipo</span>
                      <select class="form-control" name="anadir" id="anadir">
                        <option value="servicio" selected>Servicio</option>
                        <option value="material">Material</option>
                      </select>   
                    </div>
                  </h2>
                </div>
                <div class="col-lg-7 ">
                  <h2>
                    <button class="btn btn-default" {{action 'eliminarElementos'}}>Eliminar Seleccionados</button>
                    <button class="btn btn-primary" {{action 'openModalAgregarMS'}}>Añadir</button>
                    <button class="btn btn-primary" {{action 'calcularMontoTotal'}}>Calcular Total</button>
                  </h2>
                </div>

            </div>

            <div class="col-md-6 well col-pre-observ">
              <ul class="ul-presupuesto">
                <li ><b>Observaciones </b></li>
                <li>
                  <p>
                  {{presupuesto.observ}}
                  </p>
                </li>
              </ul>
            </div>
            <div class="col-md-6 well col-pre-observ">
               <dl class="dl-horizontal">
                    <dt>Subtotal 1:</dt> <dd>Bs. <span id="subtotal1">0.0</span></dd>
                    <dt>Descuento ({{presupuesto.descuento}}%):</dt> <dd>Bs. <span id="descuento">0.0</span></dd>
                    <dt>Subtotal Final:</dt> <dd>Bs. <span id="subtotalfinal">0.0</span></dd>
                    <dt>I.V.A (12%):</dt> <dd>Bs. <span id="iva">0.0</span></dd>
                    <dt>TOTAL:</dt> <dd>Bs. <span id="total">0.0</span></dd>
                </dl>
            </div>
             <div class="col-md-12 well">
              <ul class="ul-presupuesto">

                  <li ><b>Condiciones Generales </b></li>
                  <li><p>{{presupuesto.cond_g}}</p></li>

                  <li ><b>Validez de la oferta </b></li>
                  <li><p> {{presupuesto.validez_o}} Días hábiles</p></li>

                  <li ><b>Precios </b> </li>
                  <li>
                    <p>
                      {{presupuesto.cond_p}}
                    </p>
                  </li>

                  <li ><b>Tiempo de Entrega </b> </li>
                  <li><p>{{presupuesto.t_ent}}</p></li>
              </ul>
            </div>
          </div>
        </div>
      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
        <button type="submit" id="submit-button" {{action 'procesar'}} class="btn btn-primary">Procesar</button>
      </div>
    </div>
  </div>
</div>



<!-- Modal MATERIALES-->
<div class="modal fade " id="myModalMateriales" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Materiales</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
            <table class="table table-bordered table-hover">  
              <thead class="header-tabla">
                <tr>
                  <th>Código</th>
                  <th>Nombre</th>
                  <th>Descripción</th>
                  <th>Características</th>
                  <th>Marca / Modelo</th>
                  <th>Seleccionar</th>
                </tr>
              </thead>
              <tbody>
                {{#each mss as |material|}}
                <tr>
                  <td class="col-tabla">{{material.codigo}}</td>
                  <td class="col-tabla">{{material.nombre}}</td>
                  <td class="col-tabla">{{material.desc}}</td>
                  <td class="col-tabla">

                    <strong>Color:  </strong> {{material.color}} <br>
                    <strong>Serial: </strong> {{material.serial}} <br>

                  </td>
                  <td class="col-tabla">{{material.marca}} / {{material.modelo}}</td>
                  <td><input type="checkbox" value={{material.codigo}}></td>
                </tr>
                {{/each}}
              </tbody>
            </table>
        </div>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
        <button  {{action 'agregarElementos' 'material'}} class="btn btn-primary">Guardar</button>
      </div>
    </div>
  </div>
</div>





