{{logged-header-component currentName=currentName }}
 <div class="container-fluid">
        <div class="row">
        
          <div class="col-lg-2  ">
            <h2>Proyecto</h2>
          </div>
          <div class="col-lg-10">
            <div id="alertMsg" class="alert alert-dismissible" role="alert" hidden>
              <button type="button" class="close" {{action "cerrarMsg"}} aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <strong>{{msg.tipo}}</strong> {{msg.desc}}
            </div>
          </div>
        </div>

        <div class="row ">
			    {{coordinador-sidebar-component}}
          <div class="col-md-10">
          <div>

            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
              <li role="presentation" class="active"><a href="#general" aria-controls="general" role="tab" data-toggle="tab">General</a></li>
              <li role="presentation"><a href="#tecnicos" aria-controls="tecnicos" role="tab" data-toggle="tab">Técnico(s)</a></li>
              <li role="presentation"><a href="#etapas" aria-controls="etapas" role="tab" data-toggle="tab">Etapas</a></li>
              <li role="presentation"><a href="#listado" aria-controls="listado" role="tab" data-toggle="tab">Listado Materiales</a></li>
              <li role="presentation"><a href="#solicitudesmat" aria-controls="solicitudesmat" role="tab" data-toggle="tab">Solicitudes de Material</a></li>
              <li role="presentation"><a href="#presupuesto" aria-controls="presupuesto" role="tab" data-toggle="tab">Presupuesto(s)</a></li>

            </ul>

            <!-- Tab panes -->
            <div class="tab-content">

              <!-- GENERAL-->
              <div role="tabpanel" class="tab-pane active" id="general">
                

              <div class="well jumbo-color">
                <div class="row">
                  <div class="col-md-12 ">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                          <h3 class="panel-title">Detalles Proyecto <strong>{{proyecto.estatus}} </strong>

                          </h3>
                        </div>
                        <div class="panel-body">
                        
                          <div class="row">
                            <div class="col-sm-6">
                              <h4>{{proyecto.nombre}}</h4>
                              <h4><small>{{proyecto.codigo}}</small></h4>
                              <h4><small>{{proyecto.ubicacion}}</small></h4>
                              <p class="text-justify">{{proyecto.desc}}</p>
                            </div>
                            <div class="col-sm-6">
                              <div class="panel panel-default">
                                <div class="panel-body">
                                  <div class="row">
                                    <div class="col-md-6">
                                     <strong> Inicio: </strong> el {{proyecto.f_inicio}}
                                    </div>
                                    <div class="col-md-6">
                                      <strong> Estimado: </strong> para el {{proyecto.f_estimada}}
                                    </div>
                                    <div class="col-md-12">
                                    <div class="alert alert-warning text-center" role="alert">
                                        <h1> {{proyecto.t_faltante}}</h1>
                                        <small class="text-muted">según fecha estimada</small>
                                    </div>
                                    </div>
                                    <div class="col-lg-12">
                                      <h3 class="text-center">Progreso</h3>
                                      <div class="progress">
                                        <div id="progreso" class="progress-bar progress-bar-success" role="progressbar" aria-valuemin="0" aria-valuemax="100">
                                            {{progreso_porcentaje}}%
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>

                          </div>

                        </div>
                        <div class="panel-footer">
                          <div class="row">
                          <div class="col-sm-9"></div>
                            <div class="col-sm-3">
                              <button class="btn btn-primary" {{action 'openModalGeneral'}} type="submit">Editar</button>
                              <button class="btn btn-primary" {{action 'iniciarCulminarProyecto'}} type="submit">{{accion}}</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>


              </div>


              <!-- Técnicos-->
              <div role="tabpanel" class="tab-pane" id="tecnicos">
                

              <div class="well jumbo-color">
                <div class="row">
                  <div class="col-md-12 ">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                          <h3 class="panel-title">Técnicos Proyecto

                          </h3>
                        </div>
                        <div class="panel-body">
                           <div class="row">
                              <div class="col-md-12">
                                <table class="table table-bordered table-hover " id="tabla_tecnicos">  
                                  <thead class="header-tabla">
                                    <tr>
                                      <th class="col-md-2">C.I</th>
                                      <th class="col-md-4"> Nombre Técnico</th>
                                      <th class="col-md-2">Teléfono</th>
                                      <th class="col-md-2">Correo</th>
                                       <th class="col-md-1">Seleccionar</th>
                                    </tr>
                                  </thead>
                                  <tbody id="tbody" class="text-center">
                                    {{#each tecnicos as |tecnico|}}
                                    {{#if tecnico.mostrar_asociado}}
                                    <tr>
                                      <td class="col-tabla">{{tecnico.ci}}</td>
                                      <td class="col-tabla"><{{tecnico.nombre}}</td>
                                      <td class="col-tabla">{{tecnico.tlf}}</td>
                                      <td class="col-tabla">{{tecnico.correo}}</td>
                                      <td > <input type="checkbox" value="{{tecnico.ci}}"></td>
                                    </tr>
                                    {{/if}}
                                    {{/each}}
                                  </tbody>
                                </table>
                              </div>
                            </div>
                        </div>
                        <div class="panel-footer">
                          <div class="row">
                          <div class="col-sm-5"></div>

                            <div class="col-sm-3">
                              <button class="btn btn-default pull-right" {{action 'eliminarTecnicosSeleccionados'}} type="submit">Eliminar Seleccionados</button>      
                            </div>
                            <div class="col-sm-2">
                              <button class="btn btn-primary pull-right" {{action 'openModalTecnicos'}} type="submit">Agregar Técnico</button>
                            </div>
                            <div class="col-sm-2">
                              <button class="btn btn-primary pull-right" {{action 'guardarTecnicos'}} type="submit">Guardar</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

              </div>

              <!--ETAPAS-->
              <div role="tabpanel" class="tab-pane" id="etapas">
                <div class="well jumbo-color">
                <div class="row">
                  <div class="col-md-12 ">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                          <h3 class="panel-title">Etapas Proyecto</h3>
                        </div>
                        <div class="panel-body">
                        
                          <div class="row">
                            <div class="col-sm-12">
                               <table class="table table-bordered table-hover " id="tabla">  
                                <thead class="header-tabla">
                                  <tr>
                                    <th class="col-md-1">Código</th>
                                    <th >Nombre</th>
                                    <th >Estado</th>
                                    <th class="col-md-2">F. est</th>
                                    <th class="col-md-1">Reporte Detalle</th>
                                    <th class="col-md-1">Reportes</th>
                                    <th class="col-md-1">Actividades</th>
                                    <th class="col-md-1">Editar</th>
                                    <th class="col-md-1">Accion</th>
                                  </tr>
                                </thead>
                                <tbody id="tbody" class="text-center">
                                  {{#each proyecto.etapas as |etapa|}}
                                  <tr>
                                    <td class="col-tabla">{{etapa.letra}}</td>
                                    <td class="col-tabla"><{{etapa.nombre}}</td>
                                    <td class="col-tabla">{{etapa.estatus}}</td>
                                    <td class="col-tabla">
                                      {{etapa.f_est}}
                                    </td>
                                    <td >
                                    {{#if etapa.codigo_rd}}
                                      <button {{action 'openModalReporteDetalle' etapa}} class="btn btn-default btn-xs">ver</button>
                                    {{else}}
                                      N/A
                                    {{/if}}
                                    </td>
                                    <td >
                                      <button {{action 'openModalReportes' etapa}} class="btn btn-default btn-xs">ver</button>
                                    </td>
                                    <td >
                                      <button {{action 'openModalActividades' etapa}} class="btn btn-default btn-xs">ver</button>
                                    </td>
                                    <td >
                                      <button {{action 'openModalEtapa' etapa true}} class="btn btn-default btn-xs">editar</button>
                                    </td>
                                    <td >
                                      <button {{action 'accionEtapa' etapa}} class="btn btn-default btn-xs">{{etapa.accion}}</button>
                                    </td>
                                  </tr>
                                  {{/each}}
                                </tbody>
                                </table>
                            </div>
                          </div>

                        </div>
                        <div class="panel-footer">
                          <div class="row">
                            <div class="col-sm-9"></div>
                            <div class="col-sm-3">
                              <button class="btn btn-primary" {{action 'llenarReporteDetalle'}} type="submit">Llenar reporte detalle</button>
                              <button class="btn btn-primary" {{action 'openModalEtapa' null false}} type="submit">Crear Etapa</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div role="tabpanel" class="tab-pane" id="listado">
                <div class="well jumbo-color">
                  <div class="panel panel-default" id="disponibles">

                      <div class="panel-heading">
                        Materiales Disponibles
                      </div>
                       <div class="panel-body">
                            <div class="row">
                              <div class="col-md-12">
                                <table class="table table-bordered table-hover">  
                                <thead class="header-tabla">
                                  <tr>
                                    <th class="col-md-2">Código</th>
                                    <th >Nombre</th>
                                    <th >Desc</th>
                                    <th class="col-md-1">Cantidad</th>
                                  </tr>
                                </thead>
                                <tbody id="tbody" class="text-center">
                                  {{#each materiales_disponibles as |material|}}
                                  <tr>
                                    <td class="col-tabla">{{material.codigo_mat}}</td>
                                    <td class="col-tabla"><{{material.nombre}}</td>
                                    <td class="col-tabla">{{material.desc}}</td>
                                    <td class="col-tabla">{{material.cant}}</td>
                                  </tr>
                                  {{/each}}
                                </tbody>
                                </table>
                              </div>
                            </div>
                       </div>
                        <div class="panel-footer">
                        <div class="row">
                            <div class="col-sm-12">
                              <button class="btn btn-primary pull-right" {{action 'generarPDF' 'disponibles'}} type="submit">Exportar PDF</button>
                            </div>
                        </div>
                        </div>
                    </div>

                     <div class="panel panel-default" id="usados">

                      <div class="panel-heading">
                        Materiales Usados
                      </div>
                       <div class="panel-body">
                            <div class="row">
                              <div class="col-md-12">
                                <table class="table table-bordered table-hover">  
                                <thead class="header-tabla">
                                  <tr>
                                    <th class="col-md-2">Código</th>
                                    <th >Nombre</th>
                                    <th >Desc</th>
                                    <th class="col-md-1">Cantidad</th>
                                  </tr>
                                </thead>
                                <tbody id="tbody" class="text-center">
                                  {{#each materiales_usados_totales as |material|}}
                                  <tr>
                                    <td class="col-tabla">{{material.codigo_mat}}</td>
                                    <td class="col-tabla"><{{material.nombre}}</td>
                                    <td class="col-tabla">{{material.desc}}</td>
                                    <td class="col-tabla">{{material.cant}}</td>
                                  </tr>
                                  {{/each}}
                                </tbody>
                                </table>
                              </div>
                            </div>
                       </div>
                        <div class="panel-footer">
                         <div class="row">
                            <div class="col-sm-12">
                              <button class="btn btn-primary pull-right" {{action 'generarPDF' 'usados'}} type="submit">Exportar PDF</button>
                            </div>
                        </div>
                        </div>
                    </div>


                    <div class="panel panel-default" id="material_etapas">

                      <div class="panel-heading">
                        Materiales usados por etapas
                      </div>
                       <div class="panel-body">
                       {{#each materiales_usados_etapas as |etapa|}}
                           <div class="panel panel-default">

                            <div class="panel-heading">
                             <p><strong>Etapa: </strong>{{etapa.letra}}<strong>Nombre:</strong>{{etapa.nombre}}</p>
                            </div>
                             <div class="panel-body">
                                 <div class="row">
                              <div class="col-md-12">
                                <table class="table table-bordered table-hover">  
                                <thead class="header-tabla">
                                  <tr>
                                    <th class="col-md-2">Código</th>
                                    <th >Nombre</th>
                                    <th >Desc</th>
                                    <th class="col-md-1">Cantidad</th>
                                  </tr>
                                </thead>
                                <tbody id="tbody" class="text-center">
                                  {{#each etapa.materiales as |material|}}
                                  <tr>
                                    <td class="col-tabla">{{material.codigo_mat}}</td>
                                    <td class="col-tabla"><{{material.nombre}}</td>
                                    <td class="col-tabla">{{material.desc}}</td>
                                    <td class="col-tabla">{{material.cant}}</td>
                                  </tr>
                                  {{/each}}
                                </tbody>
                                </table>
                              </div>
                            </div>
                             </div>
                             </div>
                       {{/each}}
                           
                       </div>
                        <div class="panel-footer">
                         <div class="row">
                            <div class="col-sm-12">
                              <button class="btn btn-primary pull-right" {{action 'generarPDF' 'material_etapas'}} type="submit">Exportar PDF</button>
                            </div>
                        </div>
                        </div>
                    </div>
                </div>
              </div>






              <!--SOLICITUDES DE MATERIAL-->

              <div role="tabpanel" class="tab-pane" id="solicitudesmat">
                <div class="well jumbo-color">
                  <div class="panel panel-default">

                    <div class="panel-heading">
                      <div class="row">
                        <div class="col-lg-9">
                          <h4>
                            Solicitudes de material
                          </h4>
                        </div>
                        <div class="col-lg-3">
                          <h6>
                          <button class="btn btn-primary" {{action 'agregarMaterialSolicitud' }} type="submit">Agregar Material</button>
</h6>
                           <h6>
                          <button class="btn btn-primary" {{action 'guardarSolicitud' }} type="submit">Guardar Solicitud</button>
                          </h6>
                          <div class="input-group">
                {{input name="nombre" type="text" class="form-control" placeholder="Codigo material" value=prueba.codigo_mat}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>
              <div class="input-group">
                {{input name="nombre" type="text" class="form-control" placeholder="Cantidad Material" value=prueba.cant}}
                <span class="input-group-addon">{{fa-icon "money"}}</span>
              </div>
                        </div>
                      </div>
                    </div>

                    <div class="panel-body">          
                      <div class="row">
                        

                      <div class="col-md-12">
                      <table class="table table-bordered table-hover " id="tabla">  
                        <thead class="header-tabla">
                          <tr>
                            <th class="col-md-2">Técnico</th>
                            <th class="col-md-2"> Fecha Solicitud</th>
                            <th class="col-md-1">Etapa</th>
                            <th class="col-md-1">Autorizada</th>
                             <th class="col-md-1">Completada</th>
                            <th class="col-md-1">Detalles Materiales</th>
                          </tr>
                        </thead>
                        <tbody id="tbody" class="text-center">
                          {{#each proyecto.solicitudes as |solicitud|}}
                          <tr>
                            <td class="col-tabla">{{solicitud.nombre_t}}, VE-{{solicitud.ci_tecnico}}</td>
                            <td class="col-tabla"><{{solicitud.f_sol}}</td>
                             <td class="col-tabla">{{solicitud.letra_eta}}</td>
                             <td class="col-tabla">{{#if solicitud.autorizado}} Autorizada {{else}} No Autorizada{{/if}}</td>
                            <td class="col-tabla">
                              {{#if solicitud.completado}} Completada {{else}} No Completada{{/if}}
                            </td>
                            <td ><button {{action 'openModalSolicitud' solicitud}} class="btn btn-default btn-xs">Ver</button></td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                      </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <!--PRESUPUESTOS-->
              <div role="tabpanel" class="tab-pane" id="presupuesto">
                <div class="well jumbo-color">
                  <div class="panel panel-default">

                    <div class="panel-heading">
                      <div class="row">
                        <div class="col-lg-9">
                          <h4>
                            Presupuestos
                          </h4>
                        </div>
                        <div class="col-lg-3">
                          <h6>
                          <button class="btn btn-primary" {{action 'openModalPresupuesto' false}} type="submit">Solicitar Presupuesto</button>
                          </h6>
                        </div>
                      </div>
                    </div>

                    <div class="panel-body">          
                      <div class="row">
                        

                      <div class="col-md-12">
                      <table class="table table-bordered table-hover " id="tabla">  
                        <thead class="header-tabla">
                          <tr>
                            <th class="col-md-2" {{action "ordenarPor" "codigo" true}}>Código<span id="th-codigo" class="glyphicon glyphicon-chevron-down"></span></th>
                            <th {{action "ordenarPor" "desc" true}}>Descripción<span id="th-desc" class="glyphicon glyphicon-chevron-down"></span></th>
                            <th class="col-md-1"{{action "ordenarPor" "fecha" true}}>Fecha<span id="th-fecha" class="glyphicon glyphicon-chevron-down"></span></th>
                            <th class="col-md-1"{{action "ordenarPor" "estatus" true}}>Estado<span id="th-estatus" class="glyphicon glyphicon-chevron-down"></span></th>
                            <th class="col-md-1">Detalles</th>
                          </tr>
                        </thead>
                        <tbody id="tbody" class="text-center">
                          {{#each proyecto.presupuestos as |presupuesto|}}
                          <tr>
                            <td class="col-tabla">{{presupuesto.codigo}}</td>
                            <td class="col-tabla"><{{presupuesto.desc}}</td>
                            <td class="col-tabla">{{presupuesto.fecha}}</td>
                            <td class="col-tabla">{{presupuesto.estatus}}</td>
                            <td ><button {{action 'openModalPresupuesto' true presupuesto}} class="btn btn-default btn-xs">Abrir</button></td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                      </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

          </div>



          </div>
          </div>
		    </div>
</div>










<!-- Modal GENERAL-->
<div class="modal fade " id="myModalGeneral" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Edicion Detalles Generales</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
          <form  id="formulario_pg">
            <div class="row">
              <div class="col-md-12">
            <div class="form-group">
              <label>Nombre </label><small class="form-text text-muted">* </small>
              <div class="input-group">
                {{input name="nombre" id="nombre_pg" type="text" class="form-control" placeholder="Nombre del Proyecto" value=pg.nombre}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>

            </div>
          </div>

          <div class="col-md-12">
            <div class="form-group">
              <label>Descripción </label><small class="form-text text-muted">* </small>
              <div class="input-group">
                {{textarea name="desc" id="desc_pg" type="text" class="form-control" placeholder="Descripción general del proyecto" value=pg.desc}}
                <span class="input-group-addon">{{fa-icon "user"}}</span>
              </div>

            </div>
          </div>

          
          <div class="col-md-6">
          <div class="form-group">
            <label>Ubicación</label><small class="form-text text-muted"> </small>
            <div class="input-group">
              {{input name="ubicacion" type="text" class="form-control" placeholder="Ubicación del Proyecto" value=pg.ubicacion}}
              <span class="input-group-addon">{{fa-icon "map-marker"}}</span>
            </div>
          </div>
          </div>

          <div class="col-md-6">
          <div class="form-group">
            <label>Fecha estimada </label><small class="form-text text-muted">*</small>
            <div class="input-group">
              {{input name="f_est" type="date" class="form-control" placeholder="Fecha estimada de finalización" value=pg.f_est}}
              <span class="input-group-addon">{{fa-icon "calendar"}}</span>
            </div>
          </div>
          </div>

          
             <div class="col-md-12">
            <div class="form-group">
              
              <div class="input-group">
                {{input name="f_hoy" id="f_hoy_pg" type="hidden" class="form-control" value=pe.f_hoy}}

              </div>
            </div>
            </div>



        </div>
      </form>
        </div>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
        <button class="btn btn-primary" {{action 'guardarProyectoGeneral'}}>Guardar</button>
      </div>
    </div>
  </div>
</div>



<!-- Modal Tecnicos-->
<div class="modal fade " id="myModalTecnicos" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Tecnicos</h4>
      </div>
      <div class="modal-body">
          
        <div class="row">
        <div class="col-md-12">
          <form >
            <div class="row">

                <table class="table table-bordered table-hover " id="tabla_tecnicos_ss">  
                  <thead class="header-tabla">
                    <tr>
                      <th class="col-md-2">C.I</th>
                      <th class="col-md-4"> Nombre Técnico</th>
                      <th class="col-md-2">Teléfono</th>
                      <th class="col-md-2">Correo</th>
                       <th class="col-md-1">Seleccionar</th>
                    </tr>
                  </thead>
                  <tbody id="tbody" class="text-center">
                    {{#each tecnicos as |tecnico|}}
                    {{#unless tecnico.mostrar_asociado}}
                    <tr>
                      <td class="col-tabla">{{tecnico.ci}}</td>
                      <td class="col-tabla">
                      {{tecnico.nombre}}
                      </td>
                      <td class="col-tabla">{{tecnico.tlf}}</td>
                      <td class="col-tabla">{{tecnico.correo}}</td>
                      <td > <input type="checkbox" value="{{tecnico.ci}}"></td>
                    </tr>
                    {{/unless}}
                    {{/each}}
                  </tbody>
                </table>

              </div>
            </form>
          </div>
        </div>

